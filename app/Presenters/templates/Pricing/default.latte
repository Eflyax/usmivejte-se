{include priceRow, 'augmentace'}
{include priceRow, 'kyretaz'}
{include priceRowEmpty}
{include priceRow, 'zirkonovaKorunka'}
{include priceRow, 'zirkonovyMeziclen'}
{include priceRow, 'zavedeniBiomet'}
{include priceRow, 'abutmanBiomet'}
{include priceRowEmpty}
{include priceRow, 'osetreniMimoHodiny'}
</table>
<br>
<br>
<br>
<p class="price-list-header">{_cenikHygienickychPomucek}</p>
<table class="price-list">
{include priceRow, 'kartacky'}
{include priceRowEmpty}
{include priceRow, 'soloKartacek', true}
{include priceRow, 'trojKartacek', true}
{include priceRow, 'dvojKartacek', true}
{include priceRow, 'jednosvazek', true}
{include priceRow, 'detskyKartacek', true}
