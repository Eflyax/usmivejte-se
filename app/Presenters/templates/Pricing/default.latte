{include priceRow, 'airFlow'}
{include priceRow, 'rajskyPlyn'}
{include priceRowEmpty}
{include priceRow, 'rentgenMaly'}
{include priceRow, 'rentgenVelky'}
{include priceRowEmpty}
{include priceRow, 'anestezie'}
{include priceRow, 'bilaVypln'}
{include priceRow, 'calxyd'}
{include priceRow, 'biner'}
{include priceRowEmpty}
{include priceRow, 'devipasta'}
{include priceRow, 'calxydRozsireny'}
{include priceRow, 'zaplneniKanalku1'}
{include priceRow, 'zaplneniKanalku2'}
{include priceRow, 'zaplneniKanalku3'}
{include priceRow, 'zaplneniKanalku4'}
