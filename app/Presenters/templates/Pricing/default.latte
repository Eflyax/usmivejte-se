{/block}

{define #priceRow, $index, $child}
    <tr>
        <td class="item {$child ? 'child':''}">{_$index}</td>
        <td class="price">
        {ifset $priceList[$index]}
            {$priceList[$index]}
        {else}
        {/ifset}
        </td>
    </tr>
{/define}
{define #priceRowEmpty}
    <tr>
        <td class="item">&nbsp;</td><td class="price"></td>
    </tr>
{/define}